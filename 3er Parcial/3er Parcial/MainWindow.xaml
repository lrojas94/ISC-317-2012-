<Window x:Class="_3er_Parcial.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_3er_Parcial"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="600">
    <Grid>
        <TabControl x:Name="tabControl">
            <TabItem x:Name="charactersTab" Header="Characters" Height="22" VerticalAlignment="Top">
                <Frame x:Name="charactersFrame" Content="Frame" Margin="0,0,0,0" ClipToBounds="True" Background="#FFEE8383"/>
            </TabItem>
            <TabItem Header="TabItem">
            </TabItem>
        </TabControl>
    </Grid>
</Window>
